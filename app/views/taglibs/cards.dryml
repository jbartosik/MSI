<def tag="card" for="Stopien">
  <card class="cecha" param="default" merge>
    <header: param>
      <name:swiat/> - <name:cecha/>: <view:dopasowanie/>
    </header:>
  </card>
</def>

<def tag="card" for="Swiat">
  <card class="swiat" param="default" merge>
    <header: param>
      <name/> - opisany w <view with="&this.stopien_opisania(current_user)"/>
    </header:>
  </card>
</def>
